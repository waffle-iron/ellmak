{
  "name": "yadda",
  "version": "0.1.0",
  "description": "Yadda yadda yadda",
  "main": "index.js",
  "scripts": {
    "build:dev": "better-npm-run build-dev",
    "build": "better-npm-run build",
    "watch": "webpack --watch",
    "dev": "webpack-dev-server --port 3000"
  },
  "betterScripts": {
    "build-dev": {
      "command": "webpack",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "build": {
      "command": "webpack",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/CraZySacX/yadda.git"
  },
  "keywords": [
    "elm"
  ],
  "author": "Jason Ozias",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CraZySacX/yadda/issues"
  },
  "homepage": "https://github.com/CraZySacX/yadda#readme",
  "devDependencies": {
    "better-npm-run": "0.0.11",
    "debug": "^2.2.0",
    "elm-webpack-loader": "^3.0.6",
    "file-loader": "^0.9.0",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2"
  }
}
