{
  "name": "yadda",
  "version": "0.1.0",
  "description": "Yadda yadda yadda",
  "main": "index.js",
  "scripts": {
    "build:dev": "better-npm-run build-dev",
    "build": "better-npm-run build",
    "dev": "webpack-dev-server --port 3000"
  },
  "betterScripts": {
    "build-dev": {
      "command": "webpack",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "build": {
      "command": "webpack",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/CraZySacX/yadda.git"
  },
  "keywords": [
    "elm"
  ],
  "author": "Jason Ozias",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CraZySacX/yadda/issues"
  },
  "homepage": "https://github.com/CraZySacX/yadda#readme",
  "devDependencies": {
    "better-npm-run": "0.0.12",
    "copy-webpack-plugin": "^4.0.0",
    "css-loader": "^0.25.0",
    "cssnano": "^3.8.0",
    "debug": "^2.2.0",
    "elm-webpack-loader": "^3.0.6",
    "file-loader": "^0.9.0",
    "node-sass": "^3.10.1",
    "postcss-loader": "^1.0.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2",
    "yargs": "^6.3.0"
  }
}
