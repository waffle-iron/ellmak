{
  "name": "yadda",
  "version": "0.1.0",
  "description": "Yadda yadda yadda",
  "main": "index.js",
  "scripts": {
    "build:dev": "better-npm-run build-dev",
    "build": "better-npm-run build",
    "start": "better-npm-run start"
  },
  "betterScripts": {
    "build-dev": {
      "command": "webpack",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "build": {
      "command": "webpack",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "webpack-dev-server --content-base dist/ --hot --inline --progress --colors"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/CraZySacX/yadda.git"
  },
  "keywords": [
    "elm"
  ],
  "author": "Jason Ozias",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CraZySacX/yadda/issues"
  },
  "homepage": "https://github.com/CraZySacX/yadda#readme",
  "devDependencies": {
    "better-npm-run": "0.0.13",
    "bootstrap-loader": "^1.3.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.0",
    "debug": "^2.3.3",
    "elm-hot-loader": "^0.5.0",
    "elm-webpack-loader": "^3.0.6",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "imports-loader": "^0.6.5",
    "node-sass": "^3.13.0",
    "resolve-url-loader": "^1.6.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2",
    "yargs": "^6.4.0"
  },
  "dependencies": {
    "alertify.js": "git+ssh://git@github.com/CraZySacX/alertify.js.git",
    "bootstrap-sass": "^3.3.7",
    "jquery": "^3.1.1"
  }
}
